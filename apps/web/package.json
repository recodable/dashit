{
  "name": "@recodable/jumpdash",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "dev": "concurrently npm:dev:*",
    "dev:app": "vite",
    "dev:server": "docker-compose up -d && nodemon --watch 'src/server.ts' --ext 'ts,json' --exec 'DEBUG=koa* ts-node --files src/server.ts'",
    "build": "vite build",
    "knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex",
    "make:migration": "knex migrate:make -x ts",
    "db:migrate": "knex migrate:latest",
    "db:fresh": "knex migrate:rollback --all && knex migrate:latest"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa__cors": "^3.0.3",
    "@types/koa__router": "^8.0.7",
    "@types/node": "^16.3.1",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4",
    "vite": "^2.3.8",
    "vite-plugin-solid": "^2.0.0"
  },
  "dependencies": {
    "@guillotin/solid": "^0.0.5",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@rturnq/solid-auth0": "link:../../../solid-auth0",
    "date-fns": "^2.22.1",
    "fetch-intercept": "^2.4.0",
    "hotkeys-js": "^3.8.7",
    "jwks-rsa": "^2.0.4",
    "knex": "^0.95.7",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-jwt": "^4.0.1",
    "koa-logger": "^3.2.1",
    "lodash.debounce": "^4.0.8",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "solid-app-router": "^0.0.50",
    "solid-js": "^1.0.0",
    "solid-transition-group": "^0.0.8",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "whatwg-fetch": "^3.6.2"
  }
}
